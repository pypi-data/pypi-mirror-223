<odoo>
<record id="service_type_view_form" model="ir.ui.view">
    <field name="name">service.type form</field>
    <field name="model">service.type</field>
    <field name="inherit_id" ref="ssi_service.service_type_view_form" />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//group[@name='analytic_1']" position="after">
                <group name="analytic_2" colspan="4" col="2">
                    <field name="auto_create_project" />
                </group>
            </xpath>
        </data>
    </field>
</record>
</odoo>
