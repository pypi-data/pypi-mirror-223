<odoo>
<record id="ws_ra1201_account_mapping_view_search" model="ir.ui.view">
    <field name="name">ws_ra1201.account_mapping - search</field>
    <field name="model">ws_ra1201.account_mapping</field>
    <field name="mode">primary</field>
    <field name="arch" type="xml">
        <search />
    </field>
</record>

<record id="ws_ra1201_account_mapping_view_tree" model="ir.ui.view">
    <field name="name">ws_ra1201.account_mapping - tree</field>
    <field name="model">ws_ra1201.account_mapping</field>
    <field name="mode">primary</field>
    <field name="arch" type="xml">
        <tree editable="top" create="false" delete="false">
            <field name="sequence" invisible="1" />
            <field name="code" />
            <field name="account_id" />
            <field name="type_id" />
            <field name="normal_balance" />
        </tree>
    </field>
</record>

<record id="ws_ra1201_account_mapping_view_form" model="ir.ui.view">
    <field name="name">ws_ra1201.account_mapping - form</field>
    <field name="model">ws_ra1201.account_mapping</field>
    <field name="mode">primary</field>
    <field name="arch" type="xml">
        <form>
            <sheet>
                <group name="header" colspan="4" col="2">
                    <field name="sequence" invisible="1" />
                    <field name="code" />
                    <field name="account_id" />
                    <field name="type_id" />
                    <field name="normal_balance" />
                </group>
            </sheet>
        </form>
    </field>
</record>
</odoo>
