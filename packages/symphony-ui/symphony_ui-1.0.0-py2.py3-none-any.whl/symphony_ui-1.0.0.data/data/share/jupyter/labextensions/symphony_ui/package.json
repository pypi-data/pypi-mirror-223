{
  "name": "@apple/symphony-ui",
  "version": "1.0.0",
  "description": "Modular data science widgets.",
  "private": true,
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "homepage": "https://github.com/apple/ml-symphony",
  "bugs": {
    "url": "https://github.com/apple/ml-symphony/issues"
  },
  "author": "Apple",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/apple/ml-symphony"
  },
  "scripts": {
    "build": "webpack --mode=development --progress && jupyter labextension build --development=True .",
    "build:prod": "webpack --mode=production && jupyter labextension build .",
    "clean": "yarn run clean:lib && yarn run clean:nbextension && yarn run clean:labextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf symphony_ui/labextension",
    "clean:nbextension": "rimraf symphony_ui/nbextension/static/index.js",
    "lint": "eslint . --ext .ts,.tsx,.svelte --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.svelte",
    "prepack": "yarn run build:prod",
    "watch": "yarn run watch:nbextension",
    "watch:nbextension": "webpack --watch --mode=development --progress",
    "watch:labextension": "jupyter labextension watch .",
    "dev": "yarn run watch & python -m http.server --directory ./standalone 8082"
  },
  "dependencies": {
    "@apple/symphony-lib": "^1.0.0",
    "@jupyter-widgets/base": "^1.1.10 || ^2.0.0 || ^3.0.0 || ^4.0.0",
    "@tailwindcss/forms": "^0.3.3",
    "arquero": "^4.7.2",
    "autoprefixer": "^10.3.4",
    "brace": "^0.11.1",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "postcss": "^8.3.5",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.0",
    "postcss-nesting": "^11.2.0",
    "postcss-preset-env": "^6.7.0",
    "purgecss-from-svelte": "^2.0.2",
    "svelte-ace": "git+https://github.com/nateshmbhat/svelte-ace.git",
    "svelte-infinite-scroll": "^2.0.0",
    "tailwindcss": "^2.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@jupyterlab/builder": "^3.1.12",
    "@phosphor/application": "^1.6.0",
    "@phosphor/widgets": "^1.6.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "acorn": "^8.5.0",
    "commitizen": "^4.2.4",
    "css-loader": "^6.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "fs-extra": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "svelte": "^3.42.5",
    "svelte-loader": "^3.1.1",
    "svelte-preprocess": "^4.9.8",
    "ts-loader": "^9.2.5",
    "typescript": "~4.4.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^4.9.1"
  },
  "jupyterlab": {
    "extension": "lib/index",
    "outputDir": "symphony_ui/labextension/",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    },
    "_build": {
      "load": "static/remoteEntry.cde4732da3d9c31026d5.js",
      "extension": "./extension"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
