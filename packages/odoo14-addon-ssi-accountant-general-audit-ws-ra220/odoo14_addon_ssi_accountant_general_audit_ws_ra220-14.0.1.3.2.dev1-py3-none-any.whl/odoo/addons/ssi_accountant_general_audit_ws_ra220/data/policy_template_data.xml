<!-- RA.220 -->
<record id="policy_template_ra220" model="policy.template">
    <field name="name">Standard</field>
    <field name="model_id" ref="model_ws_ra220" />
    <field
                name="state_field_id"
                search="[('model_id.model','=','ws_ra220'),('name','=','state')]"
            />
    <field name="sequence" eval="100" />
    <field name="python_code">result=True</field>
</record>

<!-- RA.220: Start -->
