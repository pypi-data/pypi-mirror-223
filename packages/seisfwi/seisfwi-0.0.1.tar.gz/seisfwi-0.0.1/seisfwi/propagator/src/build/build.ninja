ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda-11.4/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=fwi -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -I/usr/local/cuda/include -I/sep/haipeng/FWI/ImageX/imagex/propagator/src/rapidjson -isystem /sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/include -isystem /sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -isystem /sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/include/TH -isystem /sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/include/THC -isystem /usr/local/cuda-11.4/include -isystem /sep/haipeng/miniconda3/envs/pytorch/include/python3.9 -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++17 -O3 -fopenmp -lpthread
post_cflags = 
cuda_cflags = -DTORCH_EXTENSION_NAME=fwi -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -I/usr/local/cuda/include -I/sep/haipeng/FWI/ImageX/imagex/propagator/src/rapidjson -isystem /sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/include -isystem /sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -isystem /sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/include/TH -isystem /sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/include/THC -isystem /usr/local/cuda-11.4/include -isystem /sep/haipeng/miniconda3/envs/pytorch/include/python3.9 -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_70,code=compute_70 -gencode=arch=compute_70,code=sm_70 --compiler-options '-fPIC' -std=c++17
cuda_post_cflags = 
cuda_dlink_post_cflags = 
ldflags = -shared -L/usr/local/cuda/lib64 -lnvrtc -lcuda -lcudart -lcufft -L/sep/haipeng/miniconda3/envs/pytorch/lib/python3.9/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda -ltorch -ltorch_python -L/usr/local/cuda-11.4/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



rule link
  command = $cxx $in $ldflags -o $out

build libAPP.o: compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/libAPP.cpp
build Parameter.o: compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/Parameter.cpp
build IsoOperator.cuda.o: cuda_compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/IsoOperator.cu
build IsoElasticKernel.cuda.o: cuda_compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/IsoElasticKernel.cu
build IsoAcousticKernel.cuda.o: cuda_compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/IsoAcousticKernel.cu
build Model.cuda.o: cuda_compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/Model.cu
build Cpml.cuda.o: cuda_compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/Cpml.cu
build utils.cuda.o: cuda_compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/utils.cu
build Survey.cuda.o: cuda_compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/Survey.cu
build Boundary.cuda.o: cuda_compile /sep/haipeng/FWI/ImageX/imagex/propagator/src/Boundary.cu



build fwi.so: link libAPP.o Parameter.o IsoOperator.cuda.o IsoElasticKernel.cuda.o IsoAcousticKernel.cuda.o Model.cuda.o Cpml.cuda.o utils.cuda.o Survey.cuda.o Boundary.cuda.o

default fwi.so

